! применить изменения: xrdb -merge ~/.Xdefaults
!=================================================

Xcursor.theme: Oxygen_Black
!Xcursor.size: 48

!-------------------------------------------------
! xterm
!-------------------------------------------------

!xterm*font:                 -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1
!xterm*font:                 -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-cp1251
!xterm*boldFont:             -*-terminus-bold-r-*-*-12-*-*-*-*-*-*-cp1251
!xterm*background:           black
!xterm*foreground:           lightgray
!xterm*ScrollBar:            false
!xterm*ScrollBar.DrawBorder: false
!xterm*eightBitInput:        false


!-------------------------------------------------
!  URxvt
!-------------------------------------------------
!-------------------------------------------------
!
! color4 - цвет панелей в mc
!
!-------------------------------------------------


URxvt.intensityStyles: false

!URxvt*transparent: true
!URxvt*shading: 30
 
URxvt.font:     xft:Ubuntu Mono:size=14
URxvt.boldFont: xft:Ubuntu Mono:size=14:style=Bold
!URxvt*font:     xft:monofur:size=15
!URxvt*boldFont: xft:monofur:size=15:bold

!Rxvt*borderLess:true
!Rxvt*scrollBar_right:true
URxvt*scrollBar:false

!URxvt*geometry: 130x35
URxvt*saveLines: 10000
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

!Rxvt*font:-*-fixed-medium-*-*-*-12-*-*-*-*-*-*-*
!Rxvt*boldFont:-*-fixed-bold-*-*-*-12-*-*-*-*-*-*-*
!URxvt*font:-*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!URxvt*boldFont:-*-terminus-bold-r-*-*-12-*-*-*-*-*-*-*
!URxvt*font:     xft:Monaco:size=15
!URxvt*boldFont: xft:Monaco:size=15:bold
!URxvt*font:     xft:Terminus:size=11
!URxvt*boldFont: xft:Terminus:size=11:bold
!URxvt*font:     xft:Ubuntu Mono:size=18
!URxvt*boldfont: xft:Ubuntu Mono:size=18:bold
!URxvt*font:     xft:DejaVu Sans Mono:size=12:bold
!URxvt*font:     xft:DejaVu Sans Mono:size=16
!URxvt*boldfont: xft:DejaVu Sans Mono:size=16:bold
!URxvt*boldFont: xft:monofur:size=15:bold
!URxvt*font: xft:Terminus:size=17
!URxvt*boldFont: xft:Terminus:size=17:bold
!URxvt*font:     xft:monofur:size=11:rgba=rgb:hinting=1:autohint=0:hintsyle=hintfull:antialias=1:lcdfilter=lcddefault
!URxvt*boldFont: xft:monofur:size=11:bold:rgba=rgb:hinting=1:autohint=0:hintsyle=hintfull:antialias=1:lcdfilter=lcddefault

!URxvt*font: -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1
URxvt*realBold: true

URxvt*jumpScroll: true
URxvt*skipScroll: true

!URxvt*cursorBlink: true
!URxvt*scrollTtyOutput: true

!URxvt*urlLauncher: /usr/bin/luakit
!URxvt.url-launcher: /usr/bin/luakit
!URxvt*underlineURLs: true
!URxvt*perl-ext-common:  default,matcher
!URxvt*matcher.button:   1
!URxvt.keysym.C-Delete:  perl:matcher:last
!URxvt.keysym.M-Delete:  perl:matcher:list
!URxvt.perl-ext-common: default,font-size
!URxvt.keysym.C-Up:     perl:font-size:increase
!URxvt.keysym.C-Down:   perl:font-size:decrease

!URxvt.urgentOnBell: True

